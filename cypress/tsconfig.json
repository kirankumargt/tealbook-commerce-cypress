{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom","es5"],
    "module": "commonjs",
    "types": ["cypress","node"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "strictNullChecks": false
  },
  "include": ["**/*.ts"]
}